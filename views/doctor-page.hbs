<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="initial-scale=1, width=device-width" />
    <link rel="stylesheet" href="/static/css/global.css" />
    <link rel="stylesheet" href="/static/css/doctor-page.css" />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;700&display=swap"
    />
  </head>
  <body>
    <div class="doctor-page">
      <img
        class="doctor-page-child"
        alt=""
        src="/static/images/rectangle-7@2x1.png"
      /><img
        class="doctor-page-child"
        alt=""
        src="/static/images/rectangle-7@2x1.png"
      /><img
        class="doctor-page-inner"
        alt=""
        src="/static/images/rectangle-61.svg"
      /><img
        class="doctor-page-child1"
        alt=""
        src="/static/images/rectangle-1@2x1.png"
      /><img
        class="doctor-page-child2"
        alt=""
        src="/static/images/rectangle-3@2x.png"
      />
      <div class="frame-div">
        <div class="frame-child11"></div>
        <button class="logout3"></button>
        <div class="history-of-your-health-checkup-parent">
          <div class="history-of-your">History Of Your Health Checkups With {{doctor.name}}</div>
          <div class="rectangle-group">
            {{#each appointment_details}}
            <div class = "rectangle-group-sub">
              <img
                class="group-child"
                alt=""
                src="/static/images/rectangle-14@2x.png"
              />
              <div class="dr-first-surname-container1">
                <ul class="dr-first">
                  {{this.doctorname}}
                </ul>
              </div>
              <div class="march-20231">{{this.date}}</div>
              <div class="small-note-like1">{{this.note}}</div>
              <button class="prescription1">Prescription</button>
              <button class="view-report1">View Report</button><br>
            </div>
            {{/each}}
      </div>
        </div>
        <a href="http://localhost:5556/landingpage"><button class="logout"></button></a>
        <div class="logo-parent">
          <button class="logo"></button>
          <a href="http://localhost:5556/index/{{user1.uid}}"><button class="dashboard1"></button></a>
          <a href="http://localhost:5556/book-appointments/{{user1.uid}}"><button class="book-appointments"></button></a>
          <a href="http://localhost:5556/appointments-history/{{user1.uid}}"><button class="past-appointments" id="pastAppointments"></button></a>
        </div>
        
        <button class="image-43"></button>
        <img class="logo-icon" alt="" src="{{doctor.image}}" />
        <div class="dr-bac-def-container">
          <p class="dr-bac-def">{{doctor.name}}</p>
          <p class="dr-bac-def">{{doctor.specialization}}</p>
          <p class="dr-bac-def">Contact Number: {{doctor.contact}}</p>
        </div>
      </div>
    </div>
  </body>
</html>
